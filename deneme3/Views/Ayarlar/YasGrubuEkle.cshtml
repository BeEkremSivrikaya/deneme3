@using EntityLayer.Tables;
@model EntityLayer.Tables.YasGrubu;
@{
    ViewData["Title"] = "YasGrubuEkle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Yaş Grubu Ekle</h1>

<form class="form-group" method="post">


    <label>Yaş Grubu Adı</label>
    @Html.TextBoxFor(x=>x.YasGrupAdi,new{@class="form-control"})



    <div class="form-group">
        <button class="btn btn-info" style="margin: 10px 10px 10px 0px;">Ekle</button>
    </div>


</form>