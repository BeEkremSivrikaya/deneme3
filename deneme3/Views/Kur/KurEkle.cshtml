@using EntityLayer.Tables;
@model EntityLayer.Tables.Kur;
@{
    ViewData["Title"] = "KurEkle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Kur Ekle</h1>
<form class="form-group" method="post">

    <label>Kur Id</label>
    @Html.TextBoxFor(x=>x.KurId,new{@class="form-control"})
    <label>Kur Adı</label>
    @Html.TextBoxFor(x=>x.KurAdi,new{@class="form-control"})
    <label>Durum</label>
    @Html.TextBoxFor(x=>x.Durum,new{@class="form-control"})
    <label>Başlangıç Zamanı</label>
    @Html.TextBoxFor(x=>x.BaslangicZamani,new{@class="form-control"})
    <label>Bitiş Zamanı</label>
    @Html.TextBoxFor(x=>x.KurBitiş,new{@class="form-control"})

    <div class="form-group">
        <button class="btn btn-info" style="margin: 10px 10px 10px 0px;">Ekle</button>
    </div>

</form>


